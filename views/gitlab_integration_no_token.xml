<!-- This page appears when there is no token recieved from github or when request to obtain github
token is not 200 -->
<odoo>
  <data>
    <template id="gitlab_integration_no_token" name="Cloud-V App Template">
      <t t-call="website.layout">
        <div id="wrap" class="oe_structure oe_empty">
          <section class="s_title pt40 pb40 o_colored_level" data-vcss="001" data-snippet="s_title"
            data-name="Title" style="background-image: none;">
            <div class="container s_allow_columns">
              <h1 style="text-align: center;">
                <font style="font-size: 62px;">No access token Received from GitLab. Please try
                  again or contact support.</font>
                <br />
              </h1>
            </div>
          </section>
        </div>
      </t>
    </template>
  </data>
</odoo>